{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "1.7.0",
        "cweagans/composer-patches": "1.6.6",
        "doctrine/annotations": "1.4.0",
        "drupal-composer/drupal-scaffold": "2.6.1",
        "drupal/auto_entitylabel": "^2.1@beta",
        "drupal/chosen": "2.8.0",
        "drupal/config_default_image": "1.0.0",
        "drupal/config_filter": "1.4.0",
        "drupal/config_split": "1.4.0",
        "drupal/console": "1.9.1",
        "drupal/consumer_image_styles": "^3.0@RC",
        "drupal/core": "8.7.6",
        "drupal/entity_browser": "2.2.0",
        "drupal/entity_embed": "1.0.0",
        "drupal/graphql": "^3.0@RC",
        "drupal/image_widget_crop": "2.2.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jsonapi_extras": "3.10.0",
        "drupal/pathauto": "1.4.0",
        "drupal/redirect": "1.4.0",
        "drupal/text_summary_options": "1.0.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/views_bulk_operations": "3.2.0",
        "drush/drush": "9.7.1",
        "harvesthq/chosen": "1.8.2",
        "vlucas/phpdotenv": "2.6.1",
        "webflo/drupal-finder": "1.2.0",
        "webmozart/path-util": "2.3.0",
        "zaporylie/composer-drupal-optimizations": "1.1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "8.8.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
